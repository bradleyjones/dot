# Images
image/jpeg; gpicview %s; test=test -n "$DISPLAY"; needsterminal;
image/jpeg; jp2a %s; copiousoutput;
image/jpg; gpicview %s; test=test -n "$DISPLAY"; needsterminal;
image/jpg; jp2a %s; copiousoutput;
image/png; gpicview %s
image/gif; gpicview %s

# PDFs
application/pdf; evince %s

# html
text/html; chromium %s ; test=test -n "$DISPLAY"; needsterminal;
text/html; w3m -I %{charset} -T text/html; copiousoutput;
