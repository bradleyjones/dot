# Images
image/jpeg; feh %s; test=test -n "$DISPLAY"; needsterminal;
image/jpeg; jp2a %s; copiousoutput;
image/jpg; feh %s; test=test -n "$DISPLAY"; needsterminal;
image/jpg; jp2a %s; copiousoutput;
image/png; feh %s
image/gif; feh %s

# PDFs
application/pdf; evince %s

# html
#text/html; chromium %s ; test=test -n DISPLAY=:0.2; needsterminal;
text/html; w3m -I %{charset} -T text/html; copiousoutput;
